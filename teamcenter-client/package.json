{
  "name": "teamcenter-client",
  "version": "1.0.0",
  "description": "Client library for interacting with Teamcenter PLM systems",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:check": "eslint . --ext .ts --check",
    "prettier": "prettier --write '**/*.{ts,js,json,md}'",
    "prettier:check": "prettier --check '**/*.{ts,js,json,md}'",
